  /* ============= 工具方法（app没法用mixins，就再写一遍了） ============= */
  isObject(item) {
    return typeof item === 'object' && !this.isArray(item)
  }
  isArray(item) {
    return Object.prototype.toString.apply(item) === '[object Array]'
  }
  isUndefined(item) {
    return typeof item === 'undefined'
  }
  /* ========================= 更新缓存信息 ======================== */
  $updateGlobalData(name, obj) {
    // 校验: globalData
    if (!this.globalData) return
    // 校验: 操作字段
    if (typeof name !== 'string' || name === '') return {}
    // 取已有信息
    const info = this.globalData[name] || {}
    // 更新缓存
    if (obj && this.isObject(obj)) {
      // Object合并第一层
      this.globalData[name] = Object.assign({}, info, obj)
    } else if (!this.isUndefined(obj)) {
      // 其他非undefined数据直接覆盖
      this.globalData[name] = obj
    }
    this.$apply && this.$apply()
    console.info(`[${obj ? 'UPDATE' : 'GET'} GlobalData ${name}]:`, this.globalData[name])
    return this.globalData[name]
  }

  //获取用户信息，并且检测用户时候注册，没有注册直接注册返回用户信息
  $getUserInfo(code, parentId = '', cb) {
    const _this = this;
    wepy.getUserInfo({
      success({userInfo}) {
        const {nickName, avatarUrl, gender} = userInfo;
        //用code换取openid
        $post('/wechat!gotWxOpenId.action', {
          code
        })
        .then(data => {
          //获取openid成功过后检测用户注册状态，如果没有注册直接注册返回用户信息！
          const openid = data.result;
          openid && $post('/async!gotMemberInfo.action', {
            openid,
            nickName,
            gender,
            head: avatarUrl,
            parentId
          })
          .then(data => {
            //清除本地缓存parentId；
            wepy.removeStorage({key: 'parentId'})
            //成功获取信息并存储为全局对象，方便其他页面调用！
